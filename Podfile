    use_frameworks!
    platform :watchos, '2.0'
    pod 'Realm',      :git => 'https://github.com/realm/realm-cocoa.git', :branch => 'swift-2.0'
    pod 'RealmSwift',      :git => 'https://github.com/realm/realm-cocoa.git', :branch => 'swift-2.0'
end
